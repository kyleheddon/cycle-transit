# Google Maps API key (server-side, for API route proxies)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Maps API key (client-side, for map rendering)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Set to "true" to use MSW mock data instead of real APIs
NEXT_PUBLIC_USE_MOCKS=true
